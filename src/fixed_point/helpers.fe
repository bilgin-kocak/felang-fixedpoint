pub fn u32_divmod(a: u32, b: u32) -> (u32, u32) {
    return (a / b, a % b)
}

pub fn u32_wide_mul(a: u32, b: u32) -> u64{
    let first: u64 = a;
    let second: u64 = b;
    let result: u64 = a * b;
    return result;
}

pub fn xor(a: bool, b: bool) -> bool {
    if a == b {
        return true;
    } else {
        return false;
    }
}